{
  "name": "open-my-calendar",
  "displayName": "Open My Calendar",
  "description": "Simply load your pre-defined calenders from VSCode command palette.",
  "version": "1.0.0",
  "publisher": "florianjosefreheis",
  "author": {
    "name": "Florian Josef Reheis"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/open-my-calendar.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:extension.addCalendar",
    "onCommand:extension.showAllCalendars",
    "onCommand:extension.openCalendar",
    "onCommand:extension.deleteCalendar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
        "command": "extension.addCalendar",
        "title": "Add Calendar",
        "category": "Calendar"
      },
      {
        "command": "extension.showAllCalendars",
        "title": "Show All Calendars",
        "category": "Calendar"
      },
      {
        "command": "extension.openCalendar",
        "title": "Open Calendar",
        "category": "Calendar"
      },
      {
        "command": "extension.deleteCalendar",
        "title": "Delete Calendar",
        "category": "Calendar"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/florianjosefreheis/open-my-calendar.git"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.31.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "valid-url": "^1.0.9"
  }
}